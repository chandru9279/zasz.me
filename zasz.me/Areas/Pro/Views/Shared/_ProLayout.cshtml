<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/ForHtmlHelpers.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/BlueprintReset.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>    
    <meta name="robots" content="noindex, nofollow" />
    <link type="image/png" href="@Url.Content("~/Content/favicon.png")" rel="shortcut icon" />
    @RenderSection("Head", false)
    <style type="text/css">
       
        .Pro
        {
            background-image: url('../../../../Content/Pro/bgpuzzle.png');
            background-repeat: repeat-y;
            background-position: center top;
            background-color: #3A3A3A;
            width: 100%;
        }
        
        .Site-In-Center
        {
            display:block;            
            width: 941px;
            margin: 40px auto;
            background-color: #3A3A3A;
        }
        
        .Elevated
        {
            min-height : 1500px;
            width: 921px;
            background-color: #FFFFFF;
        }
        
        .Elevated-Top-Shadow
        {
            background-image:url('../../../../Content/Pro/shadow-top.png');
            background-repeat:repeat-x;
            height: 20px;
            width: 926px;
            display: block;
            float: left;
        }
        
         
        .Elevated-TopRight-Shadow
        {
            background-image:url('../../../../Content/Pro/shadow-topright.png');
            background-repeat:no-repeat;
            height: 20px;
            width: 20px;
        }
        
        
        .Elevated-Right-Shadow
        {
            background-image:url('../../../../Content/Pro/shadow-right.png');
            background-repeat:repeat-y;
            width: 20px;
            float: left;
        }
        
        .Elevated-TopLeft-Padding
        {
            width: 15px;
            height: 15px;
        }
          
        .Header
        {
            background-image:url('../../../../Content/Pro/header-texture.png');
            background-repeat:repeat-x;
            width: 100%;
            height : 300px;
            color: #FFFFFF;
            font-family: Bookman Old Style;
            font-size: 30px;
            display: block;
            float: left;
        }
        
        .Sub-Header
        {
            color: #FFFFFF;
            font-family: Bookman Old Style;
            font-size: 25px;
        }
        
        .Content
        {
            color: #B8B6B4;
            width: 921px;
            background-color: #FFFFFF;
        }
            
    </style>

</head>

<body class="Pro">

    <table class="Site-In-Center">
    <tbody>
        <tr>    
            <td colspan="3"></td>
        </tr>
        <tr>
            <td class="Elevated-TopLeft-Padding"></td>
            <td class="Elevated-Top-Shadow"></td>
            <td class="Elevated-TopRight-Shadow"></td>
        </tr>
        <tr>
            <td class="Elevated" colspan="2">
            <div class="Header">
            </div>
            <div>
                @RenderBody()
            </div>        
            </td>
            <td class="Elevated-Right-Shadow"></td>
        </tr>
    </tbody>
    </table>
    
    
    
</body>
</html>


