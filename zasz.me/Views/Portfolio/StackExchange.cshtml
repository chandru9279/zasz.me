@using zasz.me.HtmlHelpers
@model zasz.me.Models.Paged<zasz.me.Models.Answer>
@{
    ViewBag.Title = "StackTacks";
}

@section Head
{
}

<div>
    <div style="clear: both;"></div>
</div>

@Html.Paging(Model.NumberOfPages)

<div class="Section">
    <ul id="Threads">
        @foreach (var answer in Model.Set)
        {
            <li>
                <a href="http://stackoverflow.com/questions/@answer.QuestionId" target="_blank">
                    @Html.Raw(answer.QuestionTitle)
                </a>
            </li>
        }
    </ul>
</div>

<div style="height: 261px">
    <img style="display: block;" src="@Url.Content("~/Content/Images/Portfolio/StackOverflow.jpg")" />
    <div style="display: block; margin-left: 650px; margin-top: -200px;">@Html.Flair()</div>
</div>