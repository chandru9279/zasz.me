@using SquishIt.Framework
@using zasz.me.ViewModels

@Html.Raw(Bundle.Css()
    .Add("~/Content/jquery-ui/jquery-ui.less")
    .Add("~/Content/jPicker/jPicker-1.1.6.css")
    .Add("~/Content/SyntaxHighlighter/shCoreDefault.css")
    .Add("~/Content/BlueprintReset.css")
    .Add("~/Content/Layout.less")
    .Render("~/Content/generated/app_#.css"))

@* Order is important, 1st jQuery then other related scripts*@
@Html.Partial("_GoogleCdnFallback", new FallbackModel("jquery/1.8.0/jquery.min.js", 
    "typeof jQuery == 'undefined'", "~/Scripts/jquery-1.8.0.min.js"))
@Html.Partial("_GoogleCdnFallback", new FallbackModel("jqueryui/1.8.23/jquery-ui.min.js", 
    "typeof jQuery.ui != 'undefined'", "~/Scripts/jquery-ui-1.8.23.min.js"))

<script type="text/javascript" src="@Url.Content("~/Scripts/ckeditor/ckeditor.js")" ></script>

@Html.Raw(
    Bundle.JavaScript()
    .AddMinified("~/Scripts/jquery.validate.min.js")
    .AddMinified("~/Scripts/jquery.validate.unobtrusive.min.js")
    .Add("~/Scripts/jpicker-1.1.6.js")
    .Add("~/Scripts/Scripts.js")
    .Render("~/Scripts/combined_#_generated.js")
    )
@Html.Raw(Bundle.JavaScript()
    .AddMinified("~/Scripts/SyntaxHighlighter/shCore.js")
    .AddMinified("~/Scripts/SyntaxHighlighter/shAutoloader.js")
    .Render("~/Scripts/SyntaxHighlighter/syntaxhighlighter_#_generated.js")
    )

<meta name="robots" content="index, follow" />
<link type="image/png" href="@Url.Content("~/Content/favicon.png")" rel="shortcut icon" />
